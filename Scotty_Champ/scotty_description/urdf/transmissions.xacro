    <joint name="knee_RR_joint">
      <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
    </joint>
    <actuator name="knee_RR_motor">
      <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
      <mechanicalReduction>${gear_reduction_knee}</mechanicalReduction>
    </actuator>
  </transmission>

  <!-- Transmissions for Rear Left (RL) -->
  <transmission name="abad_RL_joint_trans">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="abad_RL_joint">
      <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
    </joint>
    <actuator name="abad_RL_motor">
      <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
      <mechanicalReduction>${gear_reduction_abad}</mechanicalReduction>
    </actuator>
  </transmission>

  <transmission name="hip_RL_joint_trans">
