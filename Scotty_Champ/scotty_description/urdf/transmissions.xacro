    </joint>
    <actuator name="hip_RL_motor">
      <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
      <mechanicalReduction>${gear_reduction_hip}</mechanicalReduction>
    </actuator>
  </transmission>

  <transmission name="knee_RL_joint_trans">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="knee_RL_joint">
      <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
    </joint>
    <actuator name="knee_RL_motor">
      <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
      <mechanicalReduction>${gear_reduction_knee}</mechanicalReduction>
    </actuator>
  </transmission>


 </xacro:macro>
</robot>
