<?xml version="1.0"?>
<robot xmlns:xacro="http://ros.org/wiki/xacro">
 <xacro:macro name="transmissions">

  
  <!-- Gear Reduction Properties -->
  <xacro:property name="gear_reduction_abad" value="1"/>
  <xacro:property name="gear_reduction_hip" value="1"/>
  <xacro:property name="gear_reduction_knee" value="1"/>

  <!-- Transmissions for Front Right (FR) -->
  <transmission name="abad_FR_joint_trans">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="abad_FR_joint">
      <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
    </joint>
    <actuator name="abad_FR_motor">
      <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
      <mechanicalReduction>${gear_reduction_abad}</mechanicalReduction>
    </actuator>
  </transmission>

  <transmission name="hip_FR_joint_trans">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="hip_FR_joint">
