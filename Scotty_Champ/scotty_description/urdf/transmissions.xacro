      <mechanicalReduction>${gear_reduction_hip}</mechanicalReduction>
    </actuator>
  </transmission>

  <transmission name="knee_FR_joint_trans">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="knee_FR_joint">
      <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
    </joint>
    <actuator name="knee_FR_motor">
      <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
      <mechanicalReduction>${gear_reduction_knee}</mechanicalReduction>
    </actuator>
  </transmission>

  <!-- Transmissions for Front Left (FL) -->
  <transmission name="abad_FL_joint_trans">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="abad_FL_joint">
      <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
    </joint>
    <actuator name="abad_FL_motor">
      <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
      <mechanicalReduction>${gear_reduction_abad}</mechanicalReduction>
    </actuator>
  </transmission>

  <transmission name="hip_FL_joint_trans">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="hip_FL_joint">
      <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
    </joint>
    <actuator name="hip_FL_motor">
      <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
      <mechanicalReduction>${gear_reduction_hip}</mechanicalReduction>
