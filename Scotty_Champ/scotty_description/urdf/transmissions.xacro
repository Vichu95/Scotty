    <actuator name="knee_FL_motor">
      <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
      <mechanicalReduction>${gear_reduction_knee}</mechanicalReduction>
    </actuator>
  </transmission>

  <!-- Transmissions for Rear Right (RR) -->
  <transmission name="abad_RR_joint_trans">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="abad_RR_joint">
      <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
    </joint>
    <actuator name="abad_RR_motor">
      <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
      <mechanicalReduction>${gear_reduction_abad}</mechanicalReduction>
    </actuator>
  </transmission>

  <transmission name="hip_RR_joint_trans">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="hip_RR_joint">
